{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Upload-IASD-Desktop",
  "version": "2.2.0",
  "identifier": "com.gabrielkramer.uploadiasddesktop",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Upload IASD",
        "width": 1280,
        "height": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "whatsapp-bot.cjs"
    ],
    "windows": {
      "wix": {
        "bannerPath": "icons/icon.png",
        "dialogImagePath": "icons/icon.png"
      }
    }
  }
}
